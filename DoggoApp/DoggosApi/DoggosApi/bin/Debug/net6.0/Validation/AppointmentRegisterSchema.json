{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "Appointment",
  "type": "object",
  "properties": {
    "Id": {
      "type": "integer"
    },
    "Uuid": {
      "type": "integer",
      "minimum": 10000000000,
      "maximum": 99999999999
    },
    "Name": {
      "type": "string",
      "maxLength": 20
    },
    "Age": {
      "type": "integer",
      "minimum": 1,
      "maximum": 30
    },
    "Height": {
      "type": "integer",
      "minimum": 1,
      "maximum": 150
    },
    "Length": {
      "type": "integer",
      "minimum": 1,
      "maximum": 150
    },
    "Weight": {
      "type": "integer",
      "minimum": 1,
      "maximum": 100
    }
  },
  "required": ["Uuid"]
}
